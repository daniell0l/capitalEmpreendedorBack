{
  "openapi": "3.0.0",
  "info": {
    "title": "Capital Empreendedor",
    "descripton": "api de manipulação",
    "termsOfService": "http://localhost:3000/capital",
    "contact": {
      "email": "daniel_l0l@hotmail.com"
    },
    "version": "1.0.0"
  },
  "servers": [
    {
      "url": "http://localhost:3000/v1"
    }
  ],
  "paths": {
    "/users": {
      "post": {
        "summary": "Cadastro de usuario",
        "descripton": "Rota responsavel por cadastra um novo usuario",
        "tags": ["users"],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/User"
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "User": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string"
          },
          "email": {
            "type": "string"
          },
          "isActive": {
            "type": "boolean"
          },
          "phone": {
            "type": "string"
          },
          "revenue": {
            "type": "number"
          },
          "agreedTerms": {
            "type": "boolean"
          },
          "id": {
            "type": "string"
          }
        }
      }
    }
  }
}
