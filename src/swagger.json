{
  "openapi": "3.0.0",
  "info": {
    "title": " API Capital Empreendedor",
    "descripton": "api de manipulação",
    "termsOfService": "http://localhost:3000/capital",
    "contact": {
      "email": "daniel_l0l@hotmail.com"
    },
    "version": "1.0.0"
  },
  "servers": [
    {
      "url": "http://localhost:3000/v1"
    }
  ],
  "paths": {
    "/users": {
      "post": {
        "summary": "Cadastro de clientes",
        "descripton": "Rota responsavel por cadastra um novo usuario",
        "tags": ["users"],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "$ref": "#/components/schemas/User"
                },
                "example": {
                  "name": "Cliente 1",
                  "email": "cliente1@example.com",
                  "isActive": true,
                  "phone": "(19) 99999-9999",
                  "revenue": 1000000,
                  "agreedTerms": true
                }
              }
            }
          }
        },
        "responses": {
          "201": {
            "descripton": "Criado com susseso",
            "content": {
              "application/json": {
                "schema": {
                  "typo": "object",
                  "properties": {
                    "message": {
                      "typo": "string"
                    }
                  },
                  "example": {
                    "message": "Criado com susseso"
                  }
                }
              }
            }
          }
        }
      },
      "get": {
        "tags": [
          "users"
        ],
        "summary": "Lista CLientes",
        "descripton": "Lista todos os cliente",
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "type": "Array",
                  "properties": {
                    "$ref": "#/components/schemas/User"
                  },
                  "example": [
                    {
                    "name": "Cliente 1",
                    "email": "cliente1@example.com",
                    "isActive": true,
                    "phone": "(19) 99999-9999",
                    "revenue": 1000000,
                    "agreedTerms": true
                    },
                    {
                    "name": "Cliente 2",
                    "email": "cliente2@example.com",
                    "isActive": true,
                    "phone": "(19) 99999-9999",
                    "revenue": 550000,
                    "agreedTerms": false
                    }
                  ]
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
      "User": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string"
          },
          "email": {
            "type": "string"
          },
          "isActive": {
            "type": "boolean"
          },
          "phone": {
            "type": "string"
          },
          "revenue": {
            "type": "number"
          },
          "agreedTerms": {
            "type": "boolean"
          },
          "id": {
            "type": "string"
          }
        }
      }
    }
  }

